{
  "name": "A browser action that changes the page color.",
  "version": "1.0",
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "icon.png"
  },
  "manifest_version": 1,
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/", "https://*/"
  ]
} 